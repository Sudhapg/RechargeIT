CREATE TABLE Teacher (id NUMBER(19) NOT NULL, SALARY NUMBER(10) NULL, TNAME VARCHAR2(255) NULL, credential_id NUMBER(19) NULL, PRIMARY KEY (id))
CREATE TABLE Credentials (credential_id NUMBER(19) NOT NULL, PWD VARCHAR2(255) NULL, USERNAME VARCHAR2(255) NULL, TEACHER_id NUMBER(19) NULL, PRIMARY KEY (credential_id))
ALTER TABLE Teacher ADD CONSTRAINT FK_Teacher_credential_id FOREIGN KEY (credential_id) REFERENCES Credentials (credential_id)
ALTER TABLE Credentials ADD CONSTRAINT FK_Credentials_TEACHER_id FOREIGN KEY (TEACHER_id) REFERENCES Teacher (id)
